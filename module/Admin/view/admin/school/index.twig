{% extends 'layout/layout.twig' %}
{% block pageTitle %}
    {{ PageTitle ('學校資料') }}
{% endblock %}
{% block content %}

    {{ form(form) }}

    <span id="submit-btn" class="btn btn-primary">確定修改</span>

{% endblock %}

{% block inline %}
    <script>

        $(function () {

            $("#submit-btn").click(function () {

                $("#school_form").submit();
            });

        })
    </script>
{% endblock inline %}